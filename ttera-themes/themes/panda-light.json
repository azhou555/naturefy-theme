{
  "$schema": "https://zed.dev/schema/themes/v0.2.0.json",
  "name": "Panda Light",
  "author": "Theme Converter",
  "themes": [
    {
      "name": "Panda Light",
      "appearance": "light",
      "style": {
        "background": "#fefefe",
        "foreground": "#333333",
        "editor.background": "#fefefe",
        "editor.foreground": "#333333",
        "editor.gutter.background": "#fefefe",
        "editor.line_number": "#777777",
        "editor.active_line_number": "#2d5016",
        "editor.wrap_guide": "#cccccc",
        "editor.active_wrap_guide": "#cccccc",
        "panel.background": "#f8f8f8",
        "sidebar.background": "#f9f7f4",
        "tab_bar.background": "#f5f5f5",
        "tab.inactive_background": "#f5f5f5",
        "tab.active_background": "#ffffff",
        "status_bar.background": "#f5f5f5",
        "title_bar.background": "#f5f5f5",
