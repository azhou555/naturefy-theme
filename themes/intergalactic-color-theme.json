  },
  "tokenColors": [
    {
      "name": "Comments - Interstellar Dust",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#7A8394",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Strings - Nebula Green",
      "scope": [
        "string",
        "string.quoted",
        "string.template"
      ],
      "settings": {
        "foreground": "#66FF99"
      }
    },
    {
      "name": "Template String Expressions",
      "scope": [
        "meta.template.expression"
      ],
      "settings": {
        "foreground": "#F0F4F8"
      }
    },
    {
      "name": "Numbers - Golden Stellar Cores",
      "scope": [
        "constant.numeric",
        "constant.character.numeric"
      ],
      "settings": {
        "foreground": "#FFB347"
      }
    },
    {
      "name": "Built-in Constants - Stellar White",
      "scope": [
        "constant.language",
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#F0F4F8"
      }
    },
    {
      "name": "Keywords - Cosmic Blue",
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator.new",
        "keyword.other.important"
      ],
      "settings": {
        "foreground": "#5599FF"
      }
    },
    {
      "name": "Storage Types - Cosmic Magenta",
      "scope": [
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#FF44DD"
      }
    },
    {
      "name": "Function Names - Golden Amber",
      "scope": [
        "entity.name.function",
        "meta.function-call.generic",
        "support.function"
      ],
      "settings": {
        "foreground": "#FFB347"
      }
    },
    {
      "name": "Class Names - Solar Orange",
      "scope": [
        "entity.name.class",
        "entity.name.type.class",
        "support.class"
      ],
      "settings": {
        "foreground": "#FF8C42"
      }
    },
    {
      "name": "Type Names - Stellar Purple",
      "scope": [
        "entity.name.type",
        "support.type"
      ],
      "settings": {
        "foreground": "#AA88FF"
      }
    },
    {
      "name": "Variables - Stellar White",
      "scope": [
        "variable",
        "meta.object-literal.key"
      ],
      "settings": {
        "foreground": "#F0F4F8"
      }
    },
    {
      "name": "Object Properties - Electric Turquoise",
      "scope": [
        "variable.object.property",
        "variable.other.property"
      ],
      "settings": {
        "foreground": "#00E6FF"
      }
    },
    {
      "name": "Parameters - Cyan Mist",
      "scope": [
        "variable.parameter",
        "meta.parameters"
      ],
      "settings": {
        "foreground": "#4DFFFF"
      }
    },
    {
      "name": "Import/Export - Electric Turquoise",
      "scope": [
        "keyword.control.import",
        "keyword.control.export",
        "keyword.control.from",
        "keyword.control.as"
      ],
      "settings": {
        "foreground": "#00E6FF"
      }
    },
    {
      "name": "Operators - Stellar Purple",
      "scope": [
        "keyword.operator",
        "keyword.operator.arithmetic",
        "keyword.operator.assignment",
        "keyword.operator.comparison",
        "keyword.operator.logical"
      ],
      "settings": {
        "foreground": "#AA88FF"
      }
    },
    {
      "name": "Punctuation - Nebula Dark",
      "scope": [
        "punctuation",
        "meta.brace",
        "meta.bracket",
        "punctuation.separator",
        "punctuation.terminator"
      ],
      "settings": {
        "foreground": "#7A8394"
      }
    },
    {
      "name": "HTML/XML Tags - Electric Turquoise",
      "scope": [
        "entity.name.tag",
        "meta.tag",
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#00E6FF"
      }
    },
    {
      "name": "HTML/XML Attributes - Solar Orange",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#FF8C42"
      }
    },
    {
      "name": "CSS Properties - Cosmic Magenta",
      "scope": [
        "support.type.property-name.css"
      ],
      "settings": {
        "foreground": "#FF44DD"
      }
    },
    {
      "name": "CSS Values - Nebula Green",
      "scope": [
        "support.constant.property-value.css",
        "constant.other.color.rgb-value.css"
      ],
      "settings": {
        "foreground": "#66FF99"
      }
    },
    {
      "name": "CSS Selectors - Electric Turquoise",
      "scope": [
        "entity.name.tag.css",
        "entity.other.attribute-name.id.css",
        "entity.other.attribute-name.class.css"
      ],
      "settings": {
        "foreground": "#00E6FF"
      }
    },
    {
      "name": "JSON Keys - Solar Orange",
      "scope": [
        "support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#FF8C42"
      }
    },
    {
      "name": "Markdown Headings - Electric Turquoise",
      "scope": [
        "markup.heading",
        "entity.name.section.markdown"
      ],
      "settings": {
        "foreground": "#00E6FF",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Bold - Golden Amber",
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "foreground": "#FFB347",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Italic - Stellar Purple",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "foreground": "#AA88FF",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Code - Nebula Green",
      "scope": [
        "markup.inline.raw",
        "markup.fenced_code"
      ],
      "settings": {
        "foreground": "#66FF99"
      }
    },
    {
      "name": "Markdown Links - Cyan Mist",
      "scope": [
        "markup.underline.link"
      ],
      "settings": {
        "foreground": "#4DFFFF"
      }
    },
    {
      "name": "SQL Keywords - Cosmic Blue",
      "scope": [
        "keyword.other.sql"
      ],
      "settings": {
        "foreground": "#5599FF"
      }
    },
    {
      "name": "Invalid - Error Supernova",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#FF6B6B"
      }
    },
    {
      "name": "Deprecated - Warning Solar",
      "scope": [
        "invalid.deprecated"
      ],
      "settings": {
        "foreground": "#FFCC33"
      }
    }
  ]
}