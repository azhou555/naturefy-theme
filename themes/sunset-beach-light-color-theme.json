      "settings": {
        "foreground": "#E55039"
      }
    },
    {
      "scope": "storage.modifier",
      "settings": {
        "foreground": "#138496"
      }
    },
    {
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#E55039"
      }
    },
    {
      "scope": "entity.name.class",
      "settings": {
        "foreground": "#D35400"
      }
    },
    {
      "scope": "entity.name.type",
      "settings": {
        "foreground": "#2E86AB"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#2c3e50"
      }
    },
    {
      "scope": "support.function",
      "settings": {
        "foreground": "#20B2AA"
      }
    },
    {
      "scope": "support.class",
      "settings": {
        "foreground": "#9C27B0"
      }
    },
    {
      "scope": "markup.heading",
      "settings": {
        "foreground": "#E55039",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "foreground": "#D68910",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "foreground": "#2c3e50",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "meta.import, meta.export",
      "settings": {
        "foreground": "#138496"
      }
    },
    {
      "scope": "punctuation.definition.tag",
      "settings": {
        "foreground": "#138496"
      }
    },
    {
      "scope": "meta.tag, entity.name.tag",
      "settings": {
        "foreground": "#17A2B8"
      }
