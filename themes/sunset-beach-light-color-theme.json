  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#8694a6",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#17A2B8"
      }
    },
    {
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#D68910"
      }
    },
    {
