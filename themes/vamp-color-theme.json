  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#6272a4",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": ["string", "string.quoted"],
      "settings": {
        "foreground": "#f1fa8c"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#bd93f9"
      }
    },
    {
      "name": "Constants",
      "scope": ["constant.language", "constant.character"],
      "settings": {
        "foreground": "#bd93f9"
      }
    },
    {
      "name": "Keywords",
      "scope": ["keyword", "keyword.control"],
      "settings": {
        "foreground": "#8b1538",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Storage",
      "scope": ["storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#8b1538",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Function name",
      "scope": ["entity.name.function", "meta.function-call"],
      "settings": {
        "foreground": "#50fa7b"
      }
    },
    {
      "name": "Class name",
      "scope": ["entity.name.class", "entity.name.type"],
      "settings": {
        "foreground": "#8be9fd"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable"],
      "settings": {
        "foreground": "#f8f8f2"
      }
    },
    {
      "name": "Support functions",
      "scope": ["support.function"],
      "settings": {
        "foreground": "#8be9fd"
      }
    },
    {
      "name": "Support classes",
      "scope": ["support.class"],
      "settings": {
        "foreground": "#8be9fd"
      }
    },
    {
      "name": "Operator",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#f8f8f2"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#f8f8f2"
      }
    },
    {
      "name": "Tag name",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#8b1538"
      }
    },
    {
      "name": "Tag attribute",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#50fa7b"
      }
    },
    {
      "name": "Library class/type",
      "scope": ["support.type", "support.other"],
      "settings": {
        "foreground": "#8be9fd"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid"],
      "settings": {
        "foreground": "#f8f8f2",
        "background": "#8b1538"
      }
    },
    {
      "name": "JSON Key - Level 0",
      "scope": ["source.json meta.structure.dictionary.json support.type.property-name.json"],
      "settings": {
        "foreground": "#8be9fd"
      }
    },
    {
      "name": "JSON Key - Level 1",
      "scope": ["source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"],
      "settings": {
        "foreground": "#50fa7b"
      }
    },
    {
      "name": "JSON Key - Level 2",
      "scope": ["source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"],
      "settings": {
        "foreground": "#f1fa8c"
      }
    },
    {
      "name": "Markup - Italic",
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#bd93f9",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup - Bold",
      "scope": ["markup.bold"],
      "settings": {
        "foreground": "#8b1538",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup - Underline",
      "scope": ["markup.underline"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markup - Strike",
      "scope": ["markup.strike"],
      "settings": {
        "fontStyle": "strikethrough"
      }
    },
    {
      "name": "Markup - Quote",
      "scope": ["markup.quote"],
      "settings": {
        "foreground": "#6272a4",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup - Raw Block",
      "scope": ["markup.raw.block"],
      "settings": {
        "foreground": "#8be9fd"
      }
    },
    {
      "name": "Markup - Table",
      "scope": ["markup.table"],
      "settings": {
        "foreground": "#f8f8f2"
      }
    },
    {
      "name": "Markdown - Heading",
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#8b1538",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown - Link Text",
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#8be9fd"
      }
    },
    {
      "name": "Markdown - Link URL",
      "scope": ["meta.link"],
      "settings": {
        "foreground": "#bd93f9"
      }
    },
    {
      "name": "Markdown - Code Block",
      "scope": ["markup.inline.raw", "markup.fenced_code.block"],
      "settings": {
        "foreground": "#50fa7b"
      }
    },
    {
      "name": "CSS Property",
      "scope": ["support.type.property-name.css"],
      "settings": {
        "foreground": "#8be9fd"
      }
    },
    {
      "name": "CSS Value",
      "scope": ["support.constant.property-value.css", "meta.property-value.css"],
      "settings": {
        "foreground": "#f1fa8c"
      }
    },
    {
      "name": "CSS Selector",
      "scope": ["entity.name.tag.css", "entity.other.attribute-name.class.css", "entity.other.attribute-name.id.css"],
      "settings": {
        "foreground": "#50fa7b"
      }
    },
    {
      "name": "JavaScript/TypeScript Variable",
      "scope": ["variable.other.readwrite.js", "variable.other.readwrite.ts"],
      "settings": {
        "foreground": "#f8f8f2"
      }
    },
    {
      "name": "JavaScript/TypeScript This",
      "scope": ["variable.language.this"],
      "settings": {
        "foreground": "#bd93f9",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Python Self",
      "scope": ["variable.language.self.python"],
      "settings": {
        "foreground": "#bd93f9",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Python Decorator",
      "scope": ["entity.name.function.decorator.python"],
      "settings": {
        "foreground": "#50fa7b"
      }
    },
    {
      "name": "Regular Expression",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#bd93f9"
      }
    },
    {
      "name": "Template Literal",
      "scope": ["string.template"],
      "settings": {
        "foreground": "#f1fa8c"
      }
    }
  ]
}